{
  "dependencies": {
    "antd": "^3.3.3",
    "braft-editor": "^1.9.7",
    "classnames": "^2.2.5",
    "enzyme": "^3.3.0",
    "es6-promisify": "^6.0.0",
    "i18next": "^11.3.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "multer": "^1.3.0",
    "nswag": "^11.16.1",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts-ts-antd": "2.13.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/es6-promisify": "^6.0.0",
    "@types/i18next": "^8.4.3",
    "@types/jest": "^22.2.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^9.6.1",
    "@types/qs": "^6.5.1",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.4",
    "@types/react-i18next": "^7.6.1",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/redux-actions": "^2.2.4",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "typescript": "^2.8.1"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "name": "my-project",
  "private": true,
  "scripts": {
    "build": "react-scripts-ts-antd build",
    "build:all": "yarn format && yarn lint && yarn build",
    "eject": "react-scripts-ts-antd eject",
    "format": "prettier --write \"src/**/*.ts*\" \"src/**/*.less*\"",
    "lint": "tslint --project tsconfig.json",
    "start": "react-scripts-ts-antd start",
    "test": "react-scripts-ts-antd test --env=jsdom",
    "git-validate": "yarn build:all"
  },
  "version": "0.1.0",
  "lint-staged": {
    "*.{ts, tsx, js, json, css, less, md}": [
      "prettier --write",
      "git add"
    ]
  }
}
